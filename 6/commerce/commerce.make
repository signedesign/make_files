; Commerce
; commerce/commerce.make
; Provides Übercart integration.


; Include requirements
  ; includes[] = ""

; Übercart
  ; Übercart contains the original shopping system for Drupal 6.
  projects[ubercart][subdir] = "contrib"
  projects[ubercart][version] = "2.4"

; Ubercart ajax cart
  ; Omschrijving ontbreekt.
  projects[uc_ajax_cart][subdir] = "contrib"
  projects[uc_ajax_cart][version] = "2.0-beta2"

; Ubercart 2 VAT support
  ; Omschrijving ontbreekt.
  projects[uc_vat][subdir] = "contrib"
  projects[uc_vat][version] = "1.1"

; Ubercart VAT Number
  ; Omschrijving ontbreekt.
  projects[uc_vat_number][subdir] = "contrib"
  projects[uc_vat_number][version] = "1.0-beta1"

; PayPal Buttons for Ubercart
  ; Omschrijving ontbreekt.
  projects[uc_paypal_buttons][subdir] = "contrib"
  projects[uc_paypal_buttons][version] = "1.0"

; Terms of Use
  ; Omschrijving ontbreekt.
  projects[terms_of_use][subdir] = "contrib"
  projects[terms_of_use][version] = "1.12"

