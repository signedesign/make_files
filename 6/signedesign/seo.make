; SEO
; Optimization tools for better content strategy and seo

; Include requirements
  includes[seo-requirements] = "seo-requirements.make"
  ; contains contentanalysis
  
  
; Pathauto
  projects[pathauto][subdir] =  "contrib/signedesign"
  projects[pathauto][version] = "2.x-dev"
  ; requires Token 1.x-dev
; Path redirect
  projects[path_redirect][subdir] =  "contrib/signedesign"
  projects[path_redirect][version] = "1.0-rc1"
; Global redirect
  projects[globalredirect][subdir] =  "contrib/signedesign"
  projects[globalredirect][version] = "1.3-alpha1"
; Page Title
  projects[page_title][subdir] =  "contrib/signedesign"
  projects[page_title][version] = "2.4-beta1"
; Nodewords
  projects[nodewords][subdir] =  "contrib/signedesign"
  projects[nodewords][version] = "1.11"
; Content Optimizer
  projects[contentoptimizer][subdir] =  "contrib/signedesign"
  projects[contentoptimizer][version] = "2.1"
  ; requires Content Analysis
; Site Map
  projects[site_map][subdir] =  "contrib/signedesign"
  projects[site_map][version] = "1.2"
; XML Sitemap
  projects[xmlsitemap][subdir] =  "contrib/signedesign"
  projects[xmlsitemap][version] = "2.0-beta1"
; SEO Checklist
  projects[seo_checklist][subdir] =  "contrib/signedesign"
  projects[seo_checklist][version] = "3.0"
; SEO Friend
  projects[seo_friend][subdir] =  "contrib/signedesign"
  projects[seo_friend][version] = "1.0"

