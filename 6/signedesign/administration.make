; Administration
; administration.make
; Provides superuser level site-management tools.

; Include requirements
  ; includes[] = ""

; Admin
  ; Admin provides a workable administration interface.
  projects[admin][subdir] = "contrib/signedesign"
  projects[admin][version] = "2.0"

; Administration Menu
  ; Asministration Menu provides fast navigation of Drupal's entire administration environment.
  projects[admin_menu][subdir] = "contrib/signedesign"
  projects[admin_menu][version] = "3.x-dev"

; Admin Menu Hider
  ; Admin Menu Hider allows the administrator to show or hide the administration menu.
  projects[admin_menu_dropdown][subdir] = "contrib/signedesign"
  projects[admin_menu_dropdown][version] = "3.0"
  
; Admin role
  ; Admin role provides superuser rights to a regular role named administrator, allowing several superusers.
  ; projects[adminrole][subdir] = "contrib/signedesign"
  ; projects[adminrole][version] = "1.3"

; Module Info
  ; projects[moduleinfo][subdir] = "contrib/signedesign"
  ; projects[moduleinfo][version] = "1.2"

; Version Control API
  ; projects[versioncontrol][subdir] = "contrib/signedesign"
  ; projects[versioncontrol][version] = "1.0-rc2"

; Version Control API -- Git backend  
  ; projects[versioncontrol_git][subdir] = "contrib/signedesign"
  ; projects[versioncontrol_git][version] = "1.2-alpha3"
