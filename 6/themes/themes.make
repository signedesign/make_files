; Generic themes
; themes.make
; Provides generic themes to signedesign platforms and tries to remove old themes.


; Include requirements
  ; includes[] = ""

; Themes

  projects[tao][version] = "3.2"

  projects[rubik][version] = "3.0-beta2"

  projects[fusion][version] = "1.0"

  ; The type (in this case theme) is provided by drupal.org's update server's xml
  ; This will cause the projects to be recognized as themes and be placed within the theme-directory.
